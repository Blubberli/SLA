                <p>Your points:</p>
                <g:HTML ui:field="finalpoints"></g:HTML>
                </hr>
            </div>
            </div>
        </div>
        </div>
        </g:HTMLPanel>
        <g:FlexTable ui:field="table" styleName="table vocab-table table-light table-striped table-bordered"></g:FlexTable>
	
	<g:HTMLPanel ui:field="wordListPanel">
